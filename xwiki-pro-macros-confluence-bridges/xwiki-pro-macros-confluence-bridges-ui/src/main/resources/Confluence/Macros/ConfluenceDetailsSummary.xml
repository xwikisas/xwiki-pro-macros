#macro (showTags $d)
  #foreach ($tag in $d.getTagList())
    [[$services.rendering.escape($services.rendering.escape($tag, $xwiki.currentContentSyntaxId), $xwiki.currentContentSyntaxId)&gt;&gt;Main.Tags||queryString="do=viewTag&amp;tag=$escapetool.url($tag)"]]#if (!$foreach.last), #end
  #end
#end

#macro (executeMacro)
  #set ($title = $services.rendering.escape($wikimacro.parameters.firstcolumn, $xwiki.currentContentSyntaxId))
  #if (!$title)
    #set ($title = $services.localization.render('rendering.macro.detailssummary.firstcolumn'))
  #end
  #set ($results = $NULL)
  #getCQLMacroResults($xcontext.macro $results)
  #if ($results != $NULL)
    #set ($showLastModified = "$!xcontext.macro.params.showLastModified")
    #set ($showLastModified = $showLastModified.equalsIgnoreCase("true"))
    #set ($showPageLabels = "$!xcontext.macro.params.showPageLabels")
    #set ($showPageLabels = $showPageLabels.equalsIgnoreCase("true"))
    #set ($showCreator = "$!xcontext.macro.params.showCreator")
    #set ($showCreator = $showCreator.equalsIgnoreCase("true"))
    #set ($showAnyMetadata = $showLastModified || $showPageLabels || $showCreator)
    #set ($details = $services.confluence.details.getDetails("$!xcontext.macro.params.id", "$!xcontext.macro.params.headings", $results, "$!xcontext.macro.params.sortBy", "$!xcontext.macro.params.reverseSort" == "true"))
    #if ($details.size() &lt; 2)
      $services.localization.render('rendering.macro.detailssummary.noresults')
    #else
      #foreach ($row in $details)
        #if ($foreach.first)
          |=$title##
          #foreach ($cell in $row)
            |=$cell##
          #end##
          #if ($showLastModified)|=$services.localization.render('rendering.macro.detailssummary.lastModified')#end##
          #if ($showCreator)|=$services.localization.render('rendering.macro.detailssummary.creator')#end##
          #if ($showPageLabels)|=$services.localization.render('rendering.macro.detailssummary.tags')#end##

        #else
          #if ($showAnyMetadata)
            #set ($d = $xwiki.getDocument($row.get(0)))
          #end##
          #foreach ($cell in $row)
            | ((( #if ($foreach.first)[[$cell]]#{else}$cell#end ))) ##
          #end##
          #if ($showLastModified)|$xwiki.formatDate($d.getDate())#end##
          #if ($showCreator)|#if ($d.getCreator() == "XWiki.superadmin")superadmin#else[[$d.getCreator()]]#end#end##
          #if ($showPageLabels)|#showTags($d)#end##

        #end
      #end

    #end
  #end
#end
{{/velocity}}

